	UIGradient_6.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(0, 200, 255)), ColorSequenceKeypoint.new(0.46, Color3.fromRGB(255, 255, 255)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(255, 255, 255))}
	UIGradient_6.Parent = GameStatus

	StatusHolder.Name = "StatusHolder"
	StatusHolder.Parent = Frame
	StatusHolder.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	StatusHolder.Position = UDim2.new(0.0218978096, 0, 0.394339621, 0)
	StatusHolder.Size = UDim2.new(0, 393, 0, 313)
	StatusHolder.ZIndex = 0

	Corner2.CornerRadius = UDim.new(0, 4)
	Corner2.Name = "Corner2"
	Corner2.Parent = StatusHolder

	ActualGameHolder.Name = "ActualGameHolder"
	ActualGameHolder.Parent = StatusHolder
	ActualGameHolder.Active = true
	ActualGameHolder.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	ActualGameHolder.BackgroundTransparency = 1.000
	ActualGameHolder.BorderSizePixel = 0
	ActualGameHolder.Position = UDim2.new(0.0279898215, 0, 0.025559105, 0)
	ActualGameHolder.Size = UDim2.new(0, 382, 0, 305)
	ActualGameHolder.ScrollBarThickness = 5

	List.Name = "List"
	List.Parent = ActualGameHolder
	List.SortOrder = Enum.SortOrder.LayoutOrder
	
	coroutine.resume(coroutine.create(function()
		while true do wait()
			Walkspeed.Text = "Walkspeed - "..game.Players.LocalPlayer.Character.Humanoid.Walkspeed 
			Jumppower.Text = "Jumppower - "..game.Players.LocalPlayer.Character.Humanoid.JumpPower
			Health.Text = "Health - "..game.Players.LocalPlayer.Charater.Humanoid.Health.." / "..game.Players.LocalPlayer.Character.Humanoid.MaxHealth.." "..(game.Players.LocalPlayer.Character.Humanoid.Health / game.Players.LocalPlayer.Character.Humanoid.MaxHealth) * 100
		end
	end))
	
	local Mouse = game.Players.LocalPlayer:GetMouse()
	Mouse.KeyDown:Connect(function(key)
		if key == "q" then
			for i,v in pairs(statusframe) do
				v.Visible = not v.Visible
			end
		end
	end)
	
	local gameholderlib = {}
	
	function gameholderlib:AddGameStatus(status, gamename)
		local TextLabel = Instance.new("TextLabel")
		local UIGradient = Instance.new("UIGradient")
		
		TextLabel.Parent = ActualGameHolder
		TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TextLabel.BackgroundTransparency = 1.000
		TextLabel.Position = UDim2.new(0.0279898215, 0, 0, 0)
		TextLabel.Size = UDim2.new(0, 374, 0, 27)
		TextLabel.Font = Enum.Font.Gotham
		TextLabel.Text = gamename.." ".."("..status..")"
		TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
		TextLabel.TextSize = 25.000
		TextLabel.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
		TextLabel.TextWrapped = true
		TextLabel.TextXAlignment = Enum.TextXAlignment.Left

		UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(0, 200, 255)), ColorSequenceKeypoint.new(0.46, Color3.fromRGB(255, 255, 255)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(255, 255, 255))}
		UIGradient.Parent = TextLabel
	end
	
	function gameholderlib:ChangeHubType(type)
		Hubtype.Text = "Hub Type - "..type
	end
	
	return gameholderlib 
	
	end

return lib
